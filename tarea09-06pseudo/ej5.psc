Proceso ej2
	Imprimir 'Ingrese Numero A'
	A=0
	Leer A
	Imprimir 'Ingrese Numero B'
	B=0
	Leer B
	Imprimir 'Ingrese Numero C'
	C=0
	Leer C
	Imprimir 'Inicial A=',A, 'B=',B 'C=',C
	D=0
	Si A>B
		D=A
		A=B
		B=D
	FinSi
	Si A>C
		D=A
		A=C
		C=D
	FinSi
	Si B>C
		D=B
		B=C
		C=D
	FinSi	
	Imprimir 'FINAL   A=',A, 'B=',B 'C=',C
FinProceso
